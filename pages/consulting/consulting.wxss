/* pages/consulting/consulting.wxss */
.main {
  width: 100%;
  padding: 0 32rpx;
  box-sizing: border-box;

}

.content {
  margin-top: 32rpx;
  width: 100%;
  border: 1rpx solid rgba(174, 174, 174, 0.1);
  padding: 20rpx;
  box-sizing: border-box;
} 

.content-default {
  font-size: 28rpx;
  width: 100%;
  line-height: 44rpx;
}

.title {
  font-size: 32rpx;
}

.add-title {
  font-size: 32rpx;
  margin: 64rpx 0 32rpx 0;
}

.add-img {
   width: 160rpx;
  height: 160rpx;
  border-radius: 10rpx
}

.btn {
  width: 686rpx;
  line-height: 88rpx;
  font-size: 36rpx;
  color:#fff;
  background-color: #D33B33;
  margin-left: 32rpx;
  margin-top: 100rpx;
}


.photo-content {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  align-items: center;
}

.img {
  position: relative;
  width: 153rpx;
  height: 153rpx;
  margin: 20rpx 19rpx 20rpx 0;
}

.bigimg{
 width: 100%;
 height: 100%;
 border-radius: 10rpx;
}

.defaultimg{
   width: 153rpx;
   height: 153rpx;
}

.closeimg{
  position: absolute;
  top:-10rpx;
  right: -8rpx;
  width: 20rpx;
  height: 20rpx;
}