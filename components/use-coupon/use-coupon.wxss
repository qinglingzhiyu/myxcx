.coupon-list {
  position: fixed;
  display: flex;
  flex-direction: column;
  top: 234rpx;
  z-index: 9002;
  background-color: #fff;
  width: 100%;
  box-sizing: border-box;
}

.list-top {
  display: flex;
  justify-content: space-between;
  align-items: center;
 
}

.list-title {
  font-size: 40rpx;
  margin: 10rpx 0 0 39rpx;
}

.list-close {
  padding: 40rpx;
  width: 24rpx;
  height: 24rpx;  
}
.list-main {
  height: 823rpx;
  margin-top: 40rpx;
  margin-bottom: 150rpx;
}
.list {
  width: 720rpx;
  position: relative;
  height: 215rpx;
}

.list-bg {
  width: 100%;
}

.list-content {
  display: flex;
  position: absolute;
  top: 62rpx;
  justify-content: space-between;
  align-items: center;
  z-index: 9999;
  padding: 0 55rpx;
}

.money {
  color:#fff;
  font-size: 72rpx;
  margin-left: 27rpx;
}

.money>text{
  font-size: 36rpx;
}


.money-content {
  display: flex;
  flex-direction: column;
  padding-left:93rpx;
}

.money-title {
  color:#3A3A3A;
  font-size: 24rpx;
  font-weight: 700;
  width:318rpx;
}

.money-time {
  color:#C93A33;
  font-size: 24rpx;
  margin-top: 10rpx;
} 

.money-on {
  width: 60rpx;
  height: 60rpx;
  margin-left:11rpx;

}

.money-on >image {
  width: 100%;
  height: 100%;
}

.list-btn {
  position: fixed;
  bottom: 0;
  width: 100%;
  line-height: 98rpx;
  background: linear-gradient(#FF4B41, #E23A31);
  color:#fff;
  border-radius:5rpx;
}

